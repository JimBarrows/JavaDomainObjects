<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/relationship/type/form.xhtml">
	<description>
		#{partyRelatinshipTypeHome.managed ? 'Update Party
		Relationship
		Type':'Add Party Relationship Type'}
	</description>
	<param name="partyRelationshipTypeId" value="#{partyRelationshipTypeHome.id}"
		converterId="javax.faces.Long" />
	<navigation>
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/party/relationship/type/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation />
			<redirect view-id="/party/relationship/type/list.xhtml">
				<param name="partyRelationshipTypeId" value="#{partyRelationshipTypeHome.id}" />
			</redirect>
		</rule>
	</navigation>
</page>

