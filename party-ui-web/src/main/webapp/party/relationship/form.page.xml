<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/relationship/form.xhtml">
	<description>#{partyRelatinshipHome.managed
		? 'Update Party Relationship':'Add Party Relationship'}</description>
	<param name="partyRelationshipId" value="#{partyRelationshipHome.id}"
		converterId="javax.faces.Long" />
	<navigation>
		<rule if-outcome="cancel">
			<end-conversation />
			<redirect view-id="/party/relationship/list.xhtml" />
		</rule>
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/party/relationship/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation />
			<redirect view-id="/party/relationship/list.xhtml" />
		</rule>
	</navigation>
</page>

