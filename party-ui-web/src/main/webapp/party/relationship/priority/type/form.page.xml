<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/relationship/priority/type/form.xhtml">
	<description>
		#{priorityTypeHome.managed ? 'Update Relationship Priority
		Type' : 'Add Relationship Priority Type'}
	</description>
	<param name="priorityTypeId" value="#{priorityTypeHome.id}"
		converterId="javax.faces.Long" />
	<navigation>
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/party/relationship/priority/type/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation />
			<redirect view-id="/party/relationship/priority/type/list.xhtml" />
		</rule>


	</navigation>
</page>

