<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/communicationEvent/purpose/type/form.xhtml">
	<description>
		#{communicationEventPurposeHome.managed ? 'Update
		Communication
		Event Purpose' : 'Add Communication Event Purpose'}
	</description>
	<param name="communicationEventPurposeTypeId" value="#{communicationEventPurposeTypeHome.id}"
		converterId="javax.faces.Long" />
	<navigation>
		<rule if-outcome="updated">
			<end-conversation before-redirect="true" />
			<redirect view-id="/account/communicationEventPurposeTypeList.xhtml" />
		</rule>
		<rule if-outcome="persisted">
			<end-conversation before-redirect="true" />
			<redirect view-id="/account/communicationEventPurposeTypeList.xhtml" />
		</rule>
		<rule if-outcome="removed">
			<end-conversation before-redirect="true" />
			<redirect view-id="/account/communicationEventPurposeTypeList.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation before-redirect="true" />
			<redirect view-id="/account/communicationEventPurposeTypeList.xhtml" />
		</rule>


	</navigation>
</page>

