<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/case/role/type/list.xhtml">
	<description>Case Role Management</description>

	<param name="caseRoleTypeId" value="#{caseRoleTypeHome.id}"
		converterId="javax.faces.Long" />

	<navigation>
		<rule if-outcome="addCaseRoleType">
			<begin-conversation nested="true" />
			<redirect view-id="/account/caseRoleTypeForm.xhtml">
				<param name="caseRoleTypeId" value="" />
			</redirect>
		</rule>
		<rule if-outcome="editCaseRoleType">
			<begin-conversation nested="true" />
			<redirect view-id="/account/caseRoleTypeForm.xhtml">
				<param name="caseRoleTypeId" value="#{caseRoleTypeHome.id}"
					converterId="javax.faces.Long" />
			</redirect>
		</rule>
	</navigation>
</page>

