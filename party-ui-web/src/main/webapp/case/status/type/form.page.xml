<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/case/status/type/form.xhtml">
	<description>
		#{caseStatusTypeHome.managed ? 'Update Case Status' : 'Add
		Case
		Status'}
	</description>

	<param name="caseStatusTypeId" value="#{caseStatusTypeHome.id}"
		converterId="javax.faces.Long" />
	<param name="returnView" />
	<navigation>

		<rule if-outcome="removed">
			<end-conversation before-redirect="true" />
			<redirect view-id="/party/case/status/type/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation before-redirect="true" />
			<redirect view-id="/party/case/status/type/list.xhtml" />
		</rule>


	</navigation>
</page>

