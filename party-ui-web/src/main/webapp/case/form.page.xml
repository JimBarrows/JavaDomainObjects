<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/case/form.xhtml" login-required="true">
	<restrict >#{s:hasRole('nobody')}</restrict>
	<description>
		#{caseHome.managed ? 'Update Case: ' : 'Add Case'}
		#{caseHome.managed ? case.description : ''}
	</description>
	<param name="caseId" value="#{caseHome.id}" converterId="javax.faces.Long" />
	<param name="caseStatusTypeId" value="#{caseStatusTypeHome.id}"
		converterId="javax.faces.Long" />
	<param name="caseRoleTypeId" value="#{caseStatusTypeHome.id}"
		converterId="javax.faces.Long" />
	<param name="originalView" />
	<navigation>
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/party/case/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation />
			<redirect view-id="/party/case/list.xhtml" />
		</rule>
	</navigation>
</page>

