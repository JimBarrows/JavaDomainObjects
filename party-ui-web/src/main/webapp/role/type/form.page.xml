<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
	view-id="/party/role/type/form.xhtml">
	<description>
		#{roleTypeHome.managed ? 'Update Role Type' : 'Add Role
		Type'}
	</description>
	<param name="roleTypeId" value="#{roleTypeHome.id}" converterId="javax.faces.Long" />
	<param name="returnView" />
	<navigation>
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/party/role/type/list.xhtml" />
		</rule>
		<rule if-outcome="done">
			<end-conversation />
			<redirect view-id="/party/role/type/list.xhtml" />
		</rule>


	</navigation>
</page>

